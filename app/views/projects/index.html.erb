<% @projects.each do |project| %>
<div class="item-view">
  <h2><%= link_to project.title, user_project_path(current_user, project) %></h2>
    <p><%= project.description %></p>
    <div class="image-container">
      <%= image_tag(project.image) %>
    </div>
    <%= link_to 'View',user_project_path(current_user, project), :class => "button" %>
  </div>
<% end %>
